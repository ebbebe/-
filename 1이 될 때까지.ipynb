   "source": [
    "n, k = map(int, input().split())\n",
    "count = 0\n",
    "while True:\n",
    "  if n % k == 0:\n",
    "    n = n // k\n",
    "    count+=1\n",
    "  else:\n",
    "    n -= 1\n",
    "    count+=1\n",
    "  if n == 1:\n",
    "    break\n",
    "    \n",
    "print(count)\n",
    "  "
